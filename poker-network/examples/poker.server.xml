<?xml version='1.0'  encoding='iso-8859-1' ?>
<server xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="server.xsd"
  	autodeal="yes"
    simultaneous="4"
    verbose="3"
    chat="yes" >

