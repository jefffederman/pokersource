  <table name="Three" variant="holdem" betting_structure="10-15-pot-limit" seats="10" timeout="60" real_money="0" />
  <table name="Four" variant="holdem" betting_structure="15-30-no-limit" seats="10" timeout="60" real_money="0" />

  <table name="Five" variant="omaha" betting_structure="2-4-limit" seats="10" timeout="60" real_money="0" />
  <table name="Six" variant="omaha" betting_structure="10-15-limit" seats="10" timeout="60" real_money="0" />
  <table name="Seven" variant="omaha" betting_structure="10-15-pot-limit" seats="10" timeout="60" real_money="0" />
  <table name="Eight" variant="omaha" betting_structure="15-30-no-limit" seats="10" timeout="60" real_money="0" />

  <table name="Nine" variant="omaha8" betting_structure="2-4-limit" seats="10" timeout="60" real_money="0" />
  <table name="Ten" variant="omaha8" betting_structure="10-15-limit" seats="10" timeout="60" real_money="0" />
  <table name="Eleven" variant="omaha8" betting_structure="10-15-pot-limit" seats="10" timeout="60" real_money="0" />
  <table name="Twelve" variant="omaha8" betting_structure="15-30-no-limit" seats="10" timeout="60" real_money="0" />

  <table name="Thirteen" variant="7stud" betting_structure="ante-10-15-pot-limit" seats="8" timeout="60" real_money="0" />
  <table name="Fourteen" variant="7stud" betting_structure="ante-5-10-limit" seats="8" timeout="60" real_money="0" />

  <port>19380</port>
  <database name="poker3dtest" host="localhost" user="poker3dtest" password="poker3d" />
  <path>/etc/poker-engine</path> 
  <users temporary="BOT" />
</server>
