<?xml version='1.0'  encoding='iso-8859-1' ?>

<bet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:noNamespaceSchemaLocation="bet.xsd"
     name="2-4 limit" table-stakes="yes" buy-in="100" max-buy-in="100" >

    <description>Limit 2/4</description>

    <chips values="1 2 5 10 20 25 50 100 250 500 1000 2000 5000" />

    <blind small="1" big="2" change="double" frequency="10" unit="hand" />

    <all_in method="side" /> 

    <variants ids="omaha omaha8 holdem">
      <round name="pre-flop" pow_level="2" cap="3" />
      <round name="flop" pow_level="2" cap="3" />
      <round name="turn" pow_level="4" cap="3" />
      <round name="river" pow_level="4" cap="3" />
    </variants>

    <variants ids="7stud 7stud8">
      <round name="pre-flop" pow_level="2" cap="3" />
      <round name="third" pow_level="2" cap="3" />
      <round name="fourth" pow_level="2" cap="3" />
      <round name="fifth" pow_level="4" cap="3" />
      <round name="sixth" pow_level="4" cap="3" />
      <round name="seventh" pow_level="4" cap="3" />
    </variants>
</bet>

