<?xml version='1.0'  encoding='iso-8859-1' ?>

<bet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:noNamespaceSchemaLocation="bet.xsd"
     name="10-15 limit" table-stakes="yes" buy-in="500" max-buy-in="5000" >

    <description>Limit 15/30</description>

    <chips values="5 10 20 25 50 100 5000" />

    <blind small="5" big="10" />

    <all_in method="side" /> 

    <variants ids="omaha omaha8 holdem">
      <round name="pre-flop" fixed="10" cap="3" />
      <round name="flop" fixed="10" cap="3" />
      <round name="turn" fixed="15" cap="3" />
      <round name="river" fixed="15" cap="3" />
    </variants>

    <variants ids="7stud 7stud8">
      <round name="pre-flop" fixed="10" cap="3" />
      <round name="third" fixed="10" cap="3" />
      <round name="fourth" fixed="10" cap="3" />
      <round name="fifth" fixed="15" cap="3" />
      <round name="sixth" fixed="15" cap="3" />
      <round name="seventh" fixed="15" cap="3" />
    </variants>
</bet>

