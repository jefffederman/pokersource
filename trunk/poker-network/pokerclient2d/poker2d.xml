<?xml version="1.0" encoding="iso-8859-1"?>
<settings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="settings.xsd" verbose="5"
          display2d="yes" display3d="no"
          delays="true" tcptimeout="2000">
  <delays position="1" round="2" showdown="5" lag="15"/>
  <screen fullscreen="no" width="1024" height="768"/>
  <name>foobar1</name>
  <passwd>foobar</passwd>
  <remember>yes</remember>
  <muck>yes</muck>
  <auto_post>yes</auto_post>
  <chat max_chars="40" line_length="20"/>
  <servers>localhost:19378</servers>
  <sound>yes</sound>
  <tournaments real_money="n" type="sit_n_go" sort="name"/>
  <lobby real_money="n" type="holdem" sort="name"/>
  <skin url="random" outfit="random"/>
  <user path="~/.poker2d"/>
  <path>/usr/share//poker-network/poker2d /etc/poker-engine /etc/poker-network</path> 
  <data path="/usr/share/poker-network/poker2d" sounds="/usr/share/poker-network/poker2d/sounds" />
</settings>
